LeftShoulder.C0=Lerp(LeftShoulder.C0,cfMul(cf(-1,0.25+0.1*sin((sine+0.3)*2),0.15),angles(1.5707963267948966,-1.9198621771937625,1.0471975511965976)),deltaTime) 
