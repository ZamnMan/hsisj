local movinghead = getPartFromMesh(13735523220,14449801547)
local HeadWeld = getPartJoint(movinghead)

	addmode("default",{
		idle=function()
		TorsoWeld.C0=Lerp(TorsoWeld.C0,cf(0,1,0.5),deltaTime) 
